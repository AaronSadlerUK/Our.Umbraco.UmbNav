{
  "$schema": "./appsettings-schema.json",
  "Serilog": {
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Warning",
        "Microsoft.Hosting.Lifetime": "Information",
        "System": "Warning"
      }
    }
  },
  "Umbraco": {
    "CMS": {
      "Global": {
        "Id": "06a06dfa-a04a-4372-8a0a-64348ed34cf3",
        "SanitizeTinyMce": true,
        "Smtp": {
          "From": "noreply@localhost",
          "DeliveryMethod": "Network",
          "Host": "localhost"
        }
      },
      "Content": {
        "ContentVersionCleanupPolicy": {
          "EnableCleanup": true
        }
      }
    }
  }
}
